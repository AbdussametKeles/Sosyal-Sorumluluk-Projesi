@model IEnumerable<Sosyal_Sorumluluk_Projesi.Models.urunler>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}

<h2>Arşiv</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.kategoriler.kategoriAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kullanicilar.adsoyad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.memleket.memleketAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.urunAdi) 
        </th>
        <th>
            @Html.DisplayNameFor(model => model.resim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.urunİcerik)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tarih)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.kategoriler.kategoriAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.kullanicilar.adsoyad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.memleket.memleketAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.urunAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.resim)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.urunİcerik)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tarih)
        </td>
        <td>
            
            @Html.ActionLink("Delete", "Delete", new { id=item.urunID })
        </td>
    </tr>
} 
     

    @ViewData["urun_id"]="ürünler";
    @ViewData["kategori_id"] = "kategoriler";
    @ViewData["urun_adi"] = "ürün isimleri";
    @ViewData["resim"] = "resimler";
    @ViewData["urun_icerik"] = "içerikler";
    @ViewData["tarih"] = "tarihler";
    @ViewData["memleket_id"] = "memleketler";
    @ViewData["kullanici_id"] = "kullanıcılar";


</table>
